{
  "name": "note",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=notes.* REQUEST_LOG_FORMATE=common REQUEST_LOG_FILE=log.txt node ./app.mjs",
    "server1": "cross-env DEBUG=notes.* PORT=3001 node ./app.mjs",
    "server2": "cross-env DEBUG=notes.* PORT=3002 node ./app.mjs",
    "fs-start": "cross-env DEBUG=notes.* NOTES_MODEL=fs PORT=3000 node ./app.mjs",
    "fs-server1": "cross-env NOTES_MODEL=fs PORT=3001 node ./app.mjs",
    "fs-server2": "cross-env NOTES_MODEL=fs PORT=3002 node ./app.mjs",
    "fs-start2": "cross-env DEBUG=notes.* NOTES_MODEL=fs NOTES_FS_DIR=testdata PORT=3000 node ./app.mjs",
    "level-start": "cross-env DEBUG=notes.* PORT=3000 NOTES_MODEL=level node ./app.mjs",
    "sqlite3-setup": "sqlite3 chap07.sqlite3 --init models/schema-sqlite3.sql",
    "sqlite3-start": "cross-env SQLITE_FILE=chap07.sqlite3 NOTES_MODEL=sqlite3 node ./app.mjs",
    "sqlite3-server1": "cross-env SQLITE_FILE=chap07.sqlite3 PORT=3001 NOTES_MODEL=sqlite3 node ./app.mjs",
    "sqlite3-server2": "cross-env SQLITE_FILE=chap07.sqlite3 PORT=3002 NOTES_MODEL=sqlite3 node ./app.mjs",
    "sequlize-start": "cross-env DEBUG=notes.* SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize node ./app.mjs",
    "sequlize-server1": "cross-env DEBUG=notes.* SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize PORT=3001 node ./app.mjs",
    "sequlize-server2": "cross-env DEBUG=notes.* SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize PORT=3002 node ./app.mjs",
    "mysql-start": "cross-env DEBUG=notes.* SEQUELIZE_CONNECT=models/sequelize-mysql.yaml NOTES_MODEL=sequelize node ./app.mjs",
    "mysql-server1": "cross-env DEBUG=notes.* SEQUELIZE_CONNECT=models/sequelize-mysql.yaml NOTES_MODEL=sequelize PORT=3001 node ./app.mjs",
    "mysql-server2": "cross-env DEBUG=notes.* SEQUELIZE_CONNECT=models/sequelize-mysql.yaml NOTES_MODEL=sequelize PORT=3002 node ./app.mjs",
    "mongodb-start": "cross-env DEBUG=notes.* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=chap07 NOTES_MODEL=mongodb node ./app.mjs",
    "mongodb-server1": "cross-env DEBUG=notes.* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=chap07 NOTES_MODEL=mongodb PORT=3001 node ./app.mjs",
    "mongodb-server2": "cross-env DEBUG=notes.* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=chap07 NOTES_MODEL=mongodb PORT=3002 node ./app.mjs",
    "start-auth": "cross-env DEBUG=notes.* SEQUELIZE_CONNECT=models/sequelize-mysql.yaml NOTES_MODEL=sequelize USER_SERVICE_URL=http://localhost:5858 node ./app.mjs",
    "on-server": "DEBUG=notes:* SEQUELIZE_CONNECT=models/sequelize-mysql.yaml NOTES_MODEL=sequelize USER_SERVICE_URL=http://172.30.113.97:5858 PORT=3000 node ./app.mjs",
    "docker-build": "docker build -t svc-notes ."
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bootstrap": "^4.5.3",
    "connect-redis": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "feather-icons": "^4.25.0",
    "fs-extra": "^8.1.0",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "jquery": "^3.5.1",
    "js-yaml": "^3.13.1",
    "level": "^6.0.1",
    "mongodb": "^3.6.6",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport.socketio": "^3.7.0",
    "popper.js": "^1.16.1",
    "redis": "^3.1.2",
    "rotating-file-stream": "^2.1.5",
    "sequelize": "^6.6.2",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.0.2",
    "socket.io-redis": "^6.1.1",
    "sqlite3": "^5.0.2",
    "superagent": "^6.1.0"
  }
}
